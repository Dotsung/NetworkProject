<html>
  <head>
  <% include partials/head %>
  <title>gsm기숙사</title>
    <link rel="stylesheet" type="text/css" href="css/style.css">
  </head>
  <body>
    <% include partials/nav %>
    <div class="container">
      <h1>노래 신청</h1>
      <hr/>
      <table class="table table-hover">
        <thead>
          <tr>
            <th>날짜</th>
            <th>점수</th>
            <th>사유</th>
          </tr>
        </thead>
        <tbody>
        <% for(var i=0;i<rp.length;i++){ %>
        <tr>
          <td><%= formatDate(rp[i].date) %></td>
          <td><%= rp[i].point %></td>
          <td><%= rp[i].why %></td>
        </tr>
        <%}%>
      </tbody>
     </table>
    </div>
    <%=musicList%>
    <form action="/music/add" method="post">
    <p><input type="text" name=singer placeholder="가수">가수</p>
    <p><input type="text" name=title placeholder="제목">제목</p>
    
    <p><input type="submit" value="신청하기"></p>
    </form>
  </body>
</html>





